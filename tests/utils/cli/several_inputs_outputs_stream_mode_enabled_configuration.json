{
  "verbose": true,
  "stream": true,
  "input": {
      "puller1": {
          "model": "HWPCReport",
          "type": "mongodb",
          "uri": "one_uri",
          "db": "my_db",
          "collection": "my_collection",
          "name": "puller1"
      },
      "puller2": {
          "model": "HWPCReport",
          "type": "csv",
          "files": "/tmp/rapl.csv,/tmp/msr.csv",
          "name": "puller2"
      },
      "puller3": {
          "model": "HWPCReport",
          "type": "socket",
          "host": "localhost",
          "port": 1111,
          "name": "puller3"
      }
  },
  "output": {
      "one_pusher": {
          "type": "mongodb",
          "model": "PowerReport",
          "uri": "second_uri",
          "db": "my_db_result",
          "collection": "my_collection_result"
      },
      "third_pusher": {
          "type": "prometheus",
          "model": "PowerReport",
          "uri": "127.0.0.1",
          "port": 2222,
          "metric-name": "my_metric",
          "metric-description": "a description",
          "aggregation-period": 11
      },
      "fourth_pusher": {
          "type": "csv",
          "model": "PowerReport",
          "directory": "my_dir"
      },
      "fifth_pusher": {
          "type": "influxdb2",
          "model": "PowerReport",
          "uri": "127.0.0.1",
          "port": 1111,
          "db": "my_db_result_5",
          "token": "my_token",
          "org": "my_org",
          "tags": "t1,t2,t3"
      },
      "sixth": {
          "type": "opentsdb",
          "model": "PowerReport",
          "uri": "127.0.0.1",
          "port": 1111,
          "metric-name": "my metric opentsdb"
      },
      "seventh": {
          "type": "virtiofs",
          "model": "PowerReport",
          "vm-name-regexp": "vm_name$",
          "root-directory-name": "my_dir",
          "vm-directory-name-prefix": "prefix_1",
          "vm-directory-name-suffix": "suffix_1"
      },
      "eighth": {
          "type": "filedb",
          "model": "PowerReport",
          "filename": "myname.txt"
      }
  }
}


