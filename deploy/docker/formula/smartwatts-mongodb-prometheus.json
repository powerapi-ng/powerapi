{
    "verbose": true,
    "stream": true,
    "input": {
      "puller_mongodb": {
        "model": "HWPCReport",
        "type": "mongodb",
        "name": "puller_mongodb",
        "uri": "mongodb://mongodb:27017",
        "db": "db_sensor",
        "collection": "prep"
      }
    },
    "output": {
      "pusher_prometheus": {
        "type": "prometheus",
        "name": "pusher_prometheus",
        "uri": "0.0.0.0",
        "port": 9100,
        "metric_name": "powerapi_energy_consumption"
      }
    },
    "cpu-base-freq": 1900,
    "cpu-error-threshold": 2.0,
    "disable-dram-formula": true,
    "sensor-reports-frequency": 1000
  }