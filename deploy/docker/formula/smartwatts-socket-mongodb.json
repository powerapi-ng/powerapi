{
    "verbose": true,
    "stream": true,
    "input": {
      "puller_socket": {
        "model": "HWPCReport",
        "type": "socket",
        "name": "puller_socket",
        "port": 9080
      }
    },
    "output": {
      "pusher_mongodb": {
        "model": "PowerReport",
        "type": "mongodb",
        "name": "pusher_mongodb",
        "uri": "mongodb://mongodb:27017",
        "db": "db_power",
        "collection": "power"
      }
    },
    "cpu-base-freq": 1900,
    "cpu-error-threshold": 2.0,
    "disable-dram-formula": true,
    "sensor-reports-frequency": 1000
  }